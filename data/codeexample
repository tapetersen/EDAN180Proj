procedure main()
begin
	integer x = 1;
	integer y = 2;
	integer z = 3;
	bool q = true;
	bool t = true;
	bool f = false;
	x = y + z;
	y = x - z;
	z = x * y;
	y = x / z;
	q = x < y;
	q = x > y;
	q = x <= y;
	q = x >= y;
	q = x == y;
	q = x != y;
	q = (x < z or x == y);
	q = f or t;
	q = q and q;
	q = not q;
	if true then
		z = x + 1;
	fi;
	if false then
		x = y + 2;
	else
		x = y - 2;
	fi;
	
	for integer i = 1 until i == 3 do
		i = i + 1;
	od;

	x = readint;
	writeint x;
	writeln;
	
end;